{
  "name": "systrix-middleware-dashboard",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev-next": "next dev --turbopack -p 3005",
    "build": "next build --turbopack",
    "start": "NODE_ENV=production node server.js",
    "start-next": "next start -p 3005",
    "lint": "next lint",
    "links": "node show-links.js",
    "info": "node show-links.js",
    "docker:build": "docker build -t systrix-middleware-nextjs:latest .",
    "docker:run": "docker run -p 3005:3005 --env-file .env systrix-middleware-nextjs:latest",
    "docker:compose": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-auth": "^4.24.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  }
}
